  "scripts": {
    "test": "jest",
    "typeorm:migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate --dataSource ./src/infra/database/typeorm/connection.ts -p",
    "typeorm:migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource ./src/infra/database/typeorm/connection.ts",
    "typeorm:migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show --dataSource ./src/infra/database/typeorm/connection.ts",
    "typeorm:migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource ./src/infra/database/typeorm/connection.ts",
    "prisma:migration:generate": "prisma migrate dev",
    "prisma:migration:run": "prisma migrate deploy",
    "prisma:migration:show": "prisma migrate status",
    "prisma:migration:revert": "prisma migrate reset",
    "express:run": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn ./src/infra/presentation/http/express/main.ts",
    "nestjs:start:dev": "clear && ts-node-dev -r tsconfig-paths/register ./src/infra/presentation/http/nestjs/main.ts"
  },
  "prisma": {
    "schema": "src/infra/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.1.1",
